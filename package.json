{
  "name": "corbelle-backend-server",
  "version": "1.0.0",
  "author": "Tryve",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsup src/app.ts --minify --format cjs --outDir dist && tsc-alias -p tsconfig.json",
    "clean": "rm -rf ./dist",
    "serve": "cross-env node dist/app.cjs",
    "start": "npm run serve",
    "test": "jest",
    "watch-ts": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agrapheuse/Corbelle-backend.git"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.11.4",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
